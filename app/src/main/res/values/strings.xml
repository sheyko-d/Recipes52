    <string name="str_start_message_15">This is the list of items you consumed during current day. You can add more items or remove already added.</string>
    <string name="str_start_message_16">You can change fasting days at any moment.</string>
    <string name="str_start_message_17">You can track consumed items and check your diet status by clicking button below.</string>
    <string name="str_start_message_18">If you want to change some settings or start another diet you need to stop current diet cycle.</string>
